<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application title="Nouveau visualiseur thématique" logo="img/logo/geobretagne.png" help="http://geobretagne.fr" measuretools="false" exportpng="true" style="css/apps/kartenn_eolien.css"/>
    <!--<mapoptions projection="EPSG:2154" extent="145518,6726671,372058,6868691"  />-->
    <mapoptions maxzoom="19" projection="EPSG:3857" center="-220750.13768758904,6144925.57790189" zoom="8" projextent="-20037508.342789244, -20037508.342789244, 20037508.342789244, 20037508.342789244" />
  
    <baselayers style="default"><!-- style="default"||gallery" -->                  
        <baselayer  type="WMTS" id="ortho1" label="Photo aérienne actuelle" title="GéoPortail" maxscale="1000" thumbgallery="img/basemap/ortho.jpg"
            url="http://kartenn.region-bretagne.fr/wmts" layers="ORTHOIMAGERY.ORTHOPHOTOS" format="image/jpeg" visible="false" fromcapacity="false"
            attribution="&lt;a href='http://www.geoportail.fr/' target='_blank'>&lt;img src='http://api.ign.fr/geoportail/api/js/latest/theme/geoportal/img/logo_gp.gif'>&lt;/a>" style="normal" matrixset="PM" maxzoom="22"/>
        <baselayer  type="fake" id="fake" label="Carroyage" title="Carroyage" thumbgallery="img/basemap/grid.png" visible="false"  />
        <baselayer  type="OSM" id="darkmatter" label="Dark Matter" title="CartoDb" thumbgallery="img/basemap/darkmatter.png" 
            url="http://{a-c}.basemaps.cartocdn.com/dark_all/{z}/{x}/{y}.png" maxzoom="20" 
            visible="false" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="positron" label="Positron" title="CartoDb" thumbgallery="img/basemap/positron.png" 
            url="http://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png" maxzoom="20" 
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="osm1" label="OpenStreetMap" title="OpenStreetMap" thumbgallery="img/basemap/osm.png" 
            url="http://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png" 
            attribution="© MapQuest. Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'>OpenStreetMap &lt;/a>,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a>" visible="false"/>       
        <baselayer  type="WMTS" id="scan2" label="Cartes IGN" title="GéoPortail" maxscale="2000" thumbgallery="img/basemap/scan.jpg"
            url="http://kartenn.region-bretagne.fr/wmts" layers="GEOGRAPHICALGRIDSYSTEMS.MAPS" format="image/jpeg" visible="false" fromcapacity="false"
            attribution="&lt;a href='http://www.geoportail.fr/' target='_blank'>&lt;img src='http://api.ign.fr/geoportail/api/js/latest/theme/geoportal/img/logo_gp.gif'>&lt;/a>" style="normal" matrixset="PM" maxzoom="22"/>
    </baselayers>
    <olscompletion url="http://api-adresse.data.gouv.fr/search/" type="ban" attribution="La recherche d'adresse est un service proposé par la BAN"/>
    <themes>
        
        
         <theme name="Territoires"   id="territoires" icon="home">
            <layer id="rp_struct_pop_geom" name="Part de la population selon la CSP"  visible="true" tiled="false"
	                searchable="false" queryable="true" fields="nom_geo,level,p_pop,densite" aliases="nom_geo,level,p_pop,densite"
	                attributefilter="true" attributefield="level" attributevalues="Commune,EPCI,Pays"  attributelabel="Echelle" attributestylesync="true" attributefilterenabled="true"
	                infopanel=""
	                infoformat="application/vnd.ogc.gml" featurecount="5"
	                timefilter="true" timeinterval="year" timecontrol="slider" timemin="1999" timemax="2013" timevalues="1999,2008,2013"
	                style="rphab_txcs1@commune,rphab_txcs2@commune,rphab_txcs3@commune,rphab_txcs4@commune,rphab_txcs5@commune,rphab_txcs6@commune,rphab_txcs7@commune,rphab_txcs8@commune"
	                stylesalias="Agriculteurs exploitants, Artisans - Commerçants - Chefs d'entreprises, Cadres - Professions intellectuelles, Professions intermédiaires, Employés, Ouvriers, Retraités, Autres sans activité professionnelle"
	                url="http://ows.region-bretagne.fr/geoserver/rb/wms"
	                attribution="Région Bretagne" 
	                metadata=""
	                metadata-csw="">   
                    <template url="templates/demo.population.mst"></template>
              </layer>  
            <layer id="arret_ferroviaire" name="Arrêts ferroviaires régionaux"  visible="true" tiled="false" 
                    searchable="true" queryable="true" fields="nom,commune" aliases="nom,commune" secure="false"                   
                    infoformat="application/vnd.ogc.gml" featurecount="20"
                    
                    style="transports_arret_ferr_defaut, transports_arret_ferr_borne,transports_arret_ferr_guichet,transports_arret_ferr_info_dynami,transports_arret_ferr_nature,transports_arret_ferr_parking,transports_arret_ferr_velo,transports_arret_ferr_pmr"
                    stylesalias="Par défaut, Distributeur automatique de billet, Guichet, Information dynamique,Nature des arrêts ferroviaires,Parking voiture,Parking vélo, Assistance PMR"
                    legendurl="http://kartenn.region-bretagne.fr/doc/transports/icons/gare.svg"
                    url="http://ows.region-bretagne.fr/geoserver/rb/wms"
                    attribution="Région Bretagne" 
                    metadata=""
                    metadata-csw="">
                    <template url="templates/demo.arrets_fer.mst"></template>
                    <!--<template><![CDATA[
                        {{#features}}<li class="item"><b>{{nom}}</b><br/>{{code_uic}}<br/><button class="btn btn-default"/><iframe src="http://kartenn.region-bretagne.fr/ws/transports/iframe_demographie.php?insee_comm={{code_uic}}" /></li>{{/features}}
                    ]]></template>-->
              </layer>
        </theme>    
       </themes>
</config>
